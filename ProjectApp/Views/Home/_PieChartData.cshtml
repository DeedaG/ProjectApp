@model dynamic


<script type="text/javascript" src="https://www.gstatic.com/charts/loader.js"></script>
<script type="text/javascript">
google.charts.load('current', { 'packages': ['corechart'] });

    google.charts.setOnLoadCallback(drawChart);

    function drawChart() {

        var data = google.visualization.arrayToDataTable([
             @{foreach (var item in @Model.resultData)
                {
                    @Html.Raw(item);
                };
              }
        ]);

        var data2 = google.visualization.arrayToDataTable([

            ['Language', 'Days Coding'],
            @*["C#", @Model.resultData2["C#"]],
            ["Ruby", @Model.resultData2["Ruby"],
            ["React", @Model.resultData2.["React"]]*@

            ["Ruby", 1],
            ["C#", 2],
            ["React", 1]

        ]);

        var data3 = google.visualization.arrayToDataTable([

            @{foreach (var item in @Model.resultData2)
                {
                    @Html.Raw(item);
                };
              }

        ]);

          var options = {
            backgroundColor: 'transparent',
            title: 'Languages',
            titleTextStyle: {
                        style: "float:right;width:60%"
            },
            slices: [{ color: 'darkorange' }, { color: 'teal' }, { color: 'green' }, { color: 'navy' }, { color: 'purple' }, { color: 'yellow' }, ]
        };

            var options2 = {
                backgroundColor: 'transparent',
                title: 'Development Time',
                titleTextStyle: {
            style: "float:right;width:60%"
            },
            slices: [{ color: 'darkorange' }, { color: 'teal' }, { color: 'green' }, { color: 'navy' }, { color: 'purple' }, { color: 'yellow' },]
        };

            var options3 = {
            backgroundColor: 'transparent',
            title: 'Unfinished Projects',
            titleTextStyle: {
            style: "float:left;width:30%"
            },
            slices: [{ color: 'darkorange' }, { color: 'teal' }, { color: 'green' }, { color: 'navy' }, { color: 'purple' }, { color: 'yellow' },]
        };

            var chart = new google.visualization.PieChart(document.getElementById('piechart'));

            chart.draw(data, options);

            var chart2 = new google.visualization.PieChart(document.getElementById('piechart2'));
            chart2.draw(data2, options2);

            var chart3 = new google.visualization.PieChart(document.getElementById('piechart3'));

            chart3.draw(data3, options3);
    }</script>


